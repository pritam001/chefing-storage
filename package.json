{
    "name": "chefing-storage",
    "version": "1.0.0",
    "description": "Tracking programming challenge solutions @ 1. LeetCode 1. HackerEarth 1. CodeChef",
    "main": "index.js",
    "scripts": {
        "cook-py": "python3 $npm_config_problem.py",
        "cook-js": "jest $npm_config_problem.spec.js --coverage",
        "cook-ts": "jest $npm_config_problem.spec.ts",
        "view-ts": "prettier $npm_config_problem.ts $npm_config_problem.spec.ts --check && eslint $npm_config_problem.ts $npm_config_problem.spec.ts",
        "garnish-ts": "prettier $npm_config_problem.ts $npm_config_problem.spec.ts --write && eslint $npm_config_problem.ts $npm_config_problem.spec.ts --fix"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pritam001/chefing-storage.git"
    },
    "author": "Pritam Sarkar <001pritam2012@gmail.com> (https://github.com/pritam001)",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/pritam001/chefing-storage/issues"
    },
    "homepage": "https://github.com/pritam001/chefing-storage#readme",
    "dependencies": {
        "@jest/globals": "26.4.2",
        "@types/jest": "26.0.12",
        "@types/node": "14.6.2",
        "jest": "26.4.2",
        "jest-cli": "26.4.2",
        "ts-jest": "26.3.0",
        "ts-node": "9.0.0",
        "typescript": "4.0.2"
    },
    "volta": {
        "node": "14.15.3",
        "npm": "7.6.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "eslint": "^7.21.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-config-airbnb-typescript": "^14.0.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "husky": "^7.0.2",
        "lint-staged": "^11.2.0",
        "prettier": "^2.4.1"
    }
}
